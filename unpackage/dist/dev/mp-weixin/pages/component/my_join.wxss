
.dots {
	-o-text-overflow: -o-ellipsis-lastline;
	   text-overflow: -o-ellipsis-lastline;
	overflow: hidden;
	-o-text-overflow: ellipsis;
	   text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	line-clamp: 2;
	-webkit-box-orient: vertical;
	font-weight: bold;
	font-size: 0.6rem;
	margin-bottom: 5px;
}

